{
  "extends": "airbnb/base",
  "parser": "babel-eslint",
  "rules": {
    "no-multiple-empty-lines": ["error", { "max": 2 }],
    "comma-dangle": ["error", {
      "arrays": "never",
      "objects": "never",
      "imports": "never",
      "exports": "never",
      "functions": "ignore"
    }],
    "no-continue": 0,
    "no-restricted-syntax": 0,
    "import/no-named-as-default-member": 0,
    "class-methods-use-this": 0,
    "import/no-named-as-default": 0
  }
}